<div class="container">
    <div class="row">
        <h2> New Contact </h2>
        <form name="newContact" novalidate class="form-horizontal well bs-component" >
            <form-field record="contact" field="firstName" live="false" required="true"></form-field>
            <form-field record="contact" field="lastName" live="false" required="true"></form-field>
            <form-field record="contact" ng-repeat='(k,v) in contact | keyFilter: "firstName" | keyFilter: "lastName" | keyFilter: "id"' field="{{k}}"></form-field>
            <div class= "row form-group">
                <div class="col-sm-offset-2">
                    <button class="btn btn-primary" ng-click="newContact.$valid && addContact()"> Create </button>
                </div>
            </div>
        </form>
    </div>
</div>